{
  "tabWidth": 2,
  "useTabs": false,
  "semi": false,
  "singleQuote": true,
  "bracketSameLine": true,
  "editorconfig": true,
  "importOrder": ["^node:(.*)$", "<THIRD_PARTY_MODULES>", "^~/(.*)$", "^[./]"],
  "importOrderSortSpecifiers": true,
  "importOrderParserPlugins": [
    "typescript",
    "jsx",
    "topLevelAwait",
    "decorators-legacy",
    "importAssertions"
  ]
}
